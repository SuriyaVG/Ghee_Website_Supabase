{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev:server": "cross-env NODE_ENV=development tsx server/index.ts",
    "dev:client": "vite",
    "dev": "echo \"Please run 'npm run dev:server' in one terminal and 'npm run dev:client' in another, or install npm-run-all and use: npm-run-all --parallel dev:server dev:client\"",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist && (ls -l dist/public || dir dist\\public)",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:constraints": "tsx scripts/apply-production-constraints.ts",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "postbuild": "ls -l dist/public || dir dist\\public"
  },
